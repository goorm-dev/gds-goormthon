{
  "name": "@goorm-dev/gds-goormthon",
  "version": "0.0.6",
  "description": "GDS Components",
  "author": "goorm",
  "keyword": "goorm",
  "repository": {
    "type": "git",
    "url": "https://github.com/goorm-dev/gds-goormthon.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "cjs/src/index.new.js",
  "module": "es/src/index.new.js",
  "sideEffects": false,
  "src": "./src",
  "style": "goorm-storybook.css",
  "types": "es/src/index.new.d.ts",
  "files": [
    "es",
    "cjs"
  ],
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "scripts": {
    "install:all": "npm install && cd src/libs/reactstrap && git submodule update --init --recursive && npm install",
    "start": "http-server ./dist -s -p 3000",
    "build:clear": "rm -rf build && rm -rf es && rm -rf cjs",
    "build:lib": "npm run build:clear && NODE_ENV=production rollup -c ./config/rollup.config.js && npm run types",
    "types": "cp src/index.new.d.ts es/src/index.new.d.ts",
    "start:storybook": "start-storybook -c ./config/storybook -p 9009 -s ./src/static --ci",
    "build:storybook": "rm -rf ./dist && build-storybook -c ./config/storybook -s ./src/static -o ./dist",
    "lint": "eslint -c ./config/eslintrc.js --ext .js --ext .jsx --resolve-plugins-relative-to . ./src",
    "lint:report": "npm run lint -- -f json -o reports/lint/report.json",
    "lint:watch": "esw -w -c ./config/eslintrc.js --ext .js --ext .jsx ./src",
    "prettier:watch": "onchange 'src/**/*.js' 'src/**/*.jsx' 'scripts/**/*.js' 'config/**/*.js' -- prettier --config ./config/prettierrc.js --write {{changed}}",
    "git:addAll": "npm run lint && git add --all",
    "git:commit": "npm run lint && git add --all && git commit",
    "dev": "concurrently \"npm run start:storybook\" \"npm run prettier:watch\" \"npm run lint:watch\"",
    "release": "standard-version",
    "release:hotfix": "npm run release -- --release-as patch",
    "postrelease": "npm run build:lib && npm publish",
    "git:clean": "git clean -fd && git checkout .",
    "export-figma-icons": "node ./scripts/figmaAPI/getFigmaIcons.js",
    "export-figma-ExtIcons": "node ./scripts/figmaAPI/getFigmaIconsExt.js",
    "build-figma-icons": "npm run git:clean && git checkout master && git pull origin master && node ./scripts/figmaAPI/getFigmaIcons.js && npm run build:storybook && npm run git:clean",
    "build-figma-ExtIcons": "npm run git:clean && git checkout master && git pull origin master && node ./scripts/figmaAPI/getFigmaIconsExt.js && npm run build:storybook && npm run git:clean",
    "push-figma-icons": "npm run git:clean && git checkout develop && git pull origin develop && node ./scripts/figmaAPI/getFigmaIcons.js && git add . && git commit -m 'feat(icon): Figma 아이콘 업데이트' && git push origin develop",
    "push-figma-ExtIcons": "npm run git:clean && git checkout develop && git pull origin develop && node ./scripts/figmaAPI/getFigmaIconsExt.js && git add . && git commit -m 'feat(ExtIcon): Figma Extension 아이콘 업데이트' && git push origin develop"
  },
  "engines": {
    "node": ">=10.15.1"
  },
  "dependencies": {
    "@ant-design/plots": "^1.0.4",
    "@babel/runtime": "^7.10.5",
    "@mdx-js/react": "^1.6.22",
    "axios": "^0.24.0",
    "bluebird": "^3.7.2",
    "classnames": "^2.2.6",
    "dotenv": "^10.0.0",
    "lodash": "^4.17.19",
    "paths.macro": "^3.0.1",
    "prop-types": "^15.7.2",
    "react-calendar": "^3.4.0",
    "react-icon-base": "^2.1.2",
    "react-joyride": "^2.3.1",
    "react-lottie-player": "^1.4.1",
    "react-resize-detector": "^5.0.6",
    "react-table": "^7.7.0",
    "react-toastify": ">=8.1.0",
    "reactstrap": "^8.5.1"
  },
  "peerDependencies": {
    "@ant-design/plots": "^1.0.4",
    "react": ">=16.13.1",
    "react-calendar": "^3.4.0",
    "react-dom": ">=16.13.1",
    "react-joyride": "^2.3.1",
    "react-lottie-player": "^1.4.1",
    "react-toastify": ">=8.1.0",
    "reactstrap": "^8.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@emotion/core": "^10.0.28",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-docs": "^6.3.12",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-options": "^5.3.21",
    "@storybook/addon-storysource": "^6.3.12",
    "@storybook/addon-viewport": "^6.3.12",
    "@storybook/addons": "^6.5.9",
    "@storybook/react": "^6.3.12",
    "@storybook/theming": "^6.5.9",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-docgen": "^4.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "browserslist": "^4.13.0",
    "caniuse-lite": "^1.0.30001102",
    "commander": "^6.1.0",
    "concurrently": "^5.2.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-watch": "^7.0.0",
    "forever": "^3.0.0",
    "fs-extra": "^9.0.1",
    "globby": "^11.0.1",
    "http-server": "^0.12.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "onchange": "^7.0.2",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "rollup": "^2.22.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-postcss": "^3.1.3",
    "rollup-plugin-summary": "^1.3.0",
    "rollup-plugin-terser": "^6.1.0",
    "sass": "^1.49.7",
    "sass-loader": "^9.0.2",
    "standard-version": "^9.5.0",
    "storybook-addon-smart-knobs": "^6.0.2"
  }
}
